string deletion of substrings:
#include<iostream>
#include<string>
using namespace std;
int main()
{
 
 string y;
 cin>>y;

 
 for(int i=0;i<y.size();i++)
 {
  if(y[i]=='W'&&y[i+1]=='U'&&y[i+2]=='B')
  {
   y.erase(i,2);
   if(i>0)cout<<" ";
  }
  else cout<<y[i];
 }
 
}
